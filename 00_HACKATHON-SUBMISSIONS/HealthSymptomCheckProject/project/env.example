# HealthAI Environment Configuration
# Copy this file to .env and modify the values as needed

# AI API Configuration (choose one or more)
# OpenAI - Get key from https://platform.openai.com
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-3.5-turbo

# Anthropic - Get key from https://console.anthropic.com
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
VITE_ANTHROPIC_MODEL=claude-3-haiku-20240307

# Google - Get key from https://makersuite.google.com
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_MODEL=gemini-pro

# Custom API (optional)
VITE_CUSTOM_API_KEY=your_custom_api_key_here
VITE_CUSTOM_API_BASE_URL=https://your-api-endpoint.com
VITE_CUSTOM_API_MODEL=your_model_name

# App Configuration
VITE_APP_NAME=HealthAI
VITE_APP_VERSION=1.0.0

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_AI=false

# API Timeout Settings (optional)
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
